{
  "name": "DBM",
  "version": "1.9.0",
  "description": "A simple application to manage your discord bot's messages",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "dist": "build --publish=always"
  },
  "build": {
    "appId": "discordbotapp",
    "asar": "true",
    "win": {
      "target": "nsis",
      "icon": "icons/icon.ico"
    },
    "publish": {
      "provider": "github",
      "repo": "Discord-Bot-Manager",
      "owner": "MeaninglessVoid"
    }
  },
  "author": "SamuelDub",
  "devDependencies": {
    "7zip-bin-win": "^2.2.0",
    "electron": "^1.8.2",
    "electron-builder": "^19.56.0"
  },
  "dependencies": {
    "discord.js": "11.2.1",
    "electron-prompt": "^0.2.0",
    "electron-updater": "^2.20.1",
    "fast-deep-equal": "^1.0.0",
    "fs": "0.0.1-security",
    "source-map": "^0.7.0",
    "source-map-support": "^0.5.3",
    "sweetalert2": "^7.11.0"
  }
}
