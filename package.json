{
  "name": "DBM",
  "version": "1.10.0",
  "description": "A simple application to manage your discord bot's messages",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "build",
    "dist": "build --publish=always"
  },
  "build": {
    "appId": "discordbotapp",
    "asar": "true",
    "win": {
      "target": "nsis",
      "icon": "icons/icon.ico"
    },
    "publish": {
      "provider": "github",
      "repo": "Discord-Bot-Manager",
      "owner": "MeaninglessVoid"
    }
  },
  "author": "SamuelDub",
  "devDependencies": {
    "7zip-bin-win": "^2.2.0",
    "electron": "^1.8.2",
    "electron-builder": "^19.56.0",
    "fast-deep-equal": "^1.1.0",
    "source-map": "^0.7.2",
    "source-map-support": "^0.5.4"
  },
  "dependencies": {
    "discord.js": "^11.3.2",
    "electron-updater": "^2.21.8",
    "fs": "0.0.1-security",
    "moment": "^2.22.1",
    "remarkable": "^1.7.1",
    "sweetalert2": "^7.19.1"
  }
}
